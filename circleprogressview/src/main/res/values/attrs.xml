<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="board_circle_width" format="dimension" />
        <attr name="board_circle_color" format="color" />

        <attr name="progress_circle_width" format="dimension" />
        <attr name="progress_circle_color" format="color" />
        <attr name="progress_circle_color_type" format="enum">
            <enum name="fill" value="0" />
            <enum name="gradient" value="1" />
        </attr>

        <attr name="progress_circle_gradient_start_color" format="color" />
        <attr name="progress_circle_gradient_end_color" format="color" />


        <attr name="circle_padding" format="dimension" />
        <attr name="percent" format="integer" />

        <attr name="gradient_type" format="enum">
            <enum name="radial" value="0" />
            <enum name="linear" value="1" />
        </attr>
        <attr name="end_point_hollow_size" format="dimension" />
    </declare-styleable>
</resources>