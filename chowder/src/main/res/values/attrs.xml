<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScanEffect">
        <attr name="effect_color" format="color" />
        <attr name="effect_direction" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="grid_size" format="dimension" />
        <attr name="speed" format="integer" />
        <attr name="scan_width" format="dimension" />
    </declare-styleable>
</resources>